{
  "name": "termigrator-cli",
  "version": "0.1.0",
  "description": "cli for termigrator",
  "main": "./dist/index.js",
  "scripts": {
    "build": "rm -rf dist && mkdir dist && babel lib -d dist",
    "coverage": "./node_modules/.bin/babel-node ./node_modules/.bin/babel-istanbul cover ./node_modules/.bin/_mocha test/tests/*.test.js -- --timeout 30000",
    "postversion": "npm run build",
    "release": "standard-version",
    "test": "mocha --compilers js:babel-register test/tests/*.test.js --timeout 30000"
  },
  "keywords": [
    "termigrator",
    "cli"
  ],
  "author": "chris ludden",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cludden/termigrator-cli"
  },
  "dependencies": {
    "bluebird": "^3.4.1",
    "colors": "^1.1.2",
    "commander": "^2.9.0",
    "promptly": "^2.1.0",
    "retry": "^0.10.0",
    "source-map-support": "^0.4.8"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-istanbul": "^0.11.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-latest": "^6.16.0",
    "babel-register": "^6.18.0",
    "cassandra-driver": "^3.1.1",
    "chai": "^3.5.0",
    "eslint": "^3.12.2",
    "eslint-config-airbnb-base": "^11.0.0",
    "eslint-plugin-import": "^2.2.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "sinon": "^1.17.7",
    "sinon-as-promised": "^4.0.2",
    "sinon-chai": "^2.8.0",
    "standard-version": "^4.0.0",
    "termigrator": "^1.0.0"
  },
  "peerDependencies": {
    "termigrator": ">= 0.4.0"
  }
}
