{
  "extends": "airbnb-base",
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "import/no-extraneous-dependencies": [
      "error", { "devDependencies": ["**/test/**/*.js"] }
    ],
    "import/prefer-default-export": "off",
    "no-console": "off",
    "no-underscore-dangle": "off"
  }
}
